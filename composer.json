{
    "name": "iqb/cabinet",
    "description": "Cabinet - file access abstraction library.",
    "authors": [{
        "name": "Dennis Birkholz",
        "email": "cabinet@birkholz.org"
    }],
    "type": "library",
    "license": "LGPL-3",
    "autoload": {
        "psr-4": {
            "Iqb\\Cabinet\\Drawer\\": "src/Drawer/src/",
            "Iqb\\Cabinet\\GDrive\\": "src/GDrive/src/",
            "Iqb\\Cabinet\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Iqb\\Cabinet\\GDrive\\": "tests/",
            "Iqb\\Cabinet\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.0",
        "ext-igbinary": ">=2.0",
        "google/apiclient": "^2.0",
        "iqb/ecryptfs": "@dev",
        "psr/log": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6"
    }
}
